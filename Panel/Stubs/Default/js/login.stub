if($("#frm{{frmName}}Login").length > 0) {
    let rules = {
        email: {
            required: true,
            email: true,
        },
        password:{
            required: true,
            minlength: 8,
        },
    };
    ajaxRequest({
        element: "frm{{frmName}}Login",
        validation: true,
        script: "{{name}}/login",
        rules,
        afterSuccess: {
            type: "inflate_redirect_response_data",
        }
    });
}