$(document).ready(function(){
    if ($('#frmStore{{modelUp}}').length > 0) {
        let rules = {
            name: {
                required: true,
                maxlength: 253
            }
        };
        PX.ajaxRequest({
            element: 'frmStore{{modelUp}}',
            validation: true,
            script: '{{nameSpaceDown}}',
            rules,
            afterSuccess: {
                type: 'inflate_reset_response_data',
            }
        });
    }
});
