$(document).ready(function(){
    if ($('#frm{{model}}Store').length > 0) {
        let rules = {
            name: {
                required: true,
                maxlength: 253
            }
        };
        PX.ajaxRequest({
            element: 'frm{{model}}Store',
            validation: true,
            script: '{{nameDown}}/{{uri}}',
            rules,
            afterSuccess: {
                type: 'inflate_reset_response_data',
            }
        });
    }
});
