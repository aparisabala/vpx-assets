$(document).ready(function(){
    if ($('#frm{{model}}Update').length > 0) {
        let rules = {
            name: {
                required: true,
                maxlength: 253
            }
        };
        PX.ajaxRequest({
            element: 'frm{{model}}Update',
            validation: true,
            script: '{{nameDown}}/{{uri}}/update/'+$("#patch_id").val(),
            rules,
            afterSuccess: {
                type: 'inflate_response_data',
            }
        });
    }
});
